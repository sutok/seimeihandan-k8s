# 環境変数設定ファイルの例
# このファイルを .env にコピーして使用してください

# GCP設定
PROJECT_ID=your_project_id_here
CLUSTER_NAME=your_cluster_name_here
REGION=asia-northeast1
ZONE=asia-northeast1-a

# アプリケーション設定
APP_NAME=seimei-handan
NAMESPACE=seimei-handan

# Cloud Functions設定
FUNCTION_NAME=seimei-handan-api
FUNCTION_URL=https://asia-northeast1-your_project_id_here.cloudfunctions.net/seimei-handan-api

# Cloud Storage設定
BUCKET_NAME=seimei-handan-bucket

# ドメイン設定
DOMAIN_NAME=seimei-handan.shinyudo.com

# セキュリティ設定
ALLOWED_IPS=your_allowed_ips_here
